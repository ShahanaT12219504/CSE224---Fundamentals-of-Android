fun main() {
     var name:String = 
    """
    Name
    Name
    Name
    Name
    Name
    """
    println(name)

    var name1: String = "Name\nName\nName\nName\n"
    println(name1)

    var name2: String = """
    Name
     Name
      Name
       Name """
    println(name2)
  
    var name3: String = """
           Name
    Name           Name
           Name """
    println(name3)
  
}
